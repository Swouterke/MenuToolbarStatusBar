<Window x:Class="MenuToolbarStatusBar.BrushesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MenuToolbarStatusBar"
        mc:Ignorable="d"
        Title="BrushesWindow" Height="160" Width="210">
    <Window.Resources>
        <Color x:Key="paarsColor" A="255" R="170" G="40" B="165"/>
        <SolidColorBrush x:Key="paarsBrush" Color="{StaticResource paarsColor}"/>
    </Window.Resources>
    <StackPanel Background="{StaticResource paarsBrush}">
        
    </StackPanel>
</Window>
